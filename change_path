#!/usr/bin/env bash
PWD=`pwd`

rm ${HOME}/.vim
rm ${HOME}/.vimrc

ln -sf ${PWD} ${HOME}/.vim
ln -sf ${PWD}/vimrc.pathogen ${HOME}/.vimrc
ln -sf ${PWD}/config/tern-config ${HOME}/.tern-config


