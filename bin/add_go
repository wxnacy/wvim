#!/usr/bin/env bash

go get -u github.com/mdempsky/gocode

cp ${GOPATH}/src/github.com/mdempsky/gocode/vim/autoload/gocomplete.vim ~/.vim/bundle/gocode/autoload
cp ${GOPATH}/src/github.com/mdempsky/gocode/vim/ftplugin/go/gocomplete.vim ~/.vim/bundle/gocode/ftplugin/go

